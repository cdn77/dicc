    },
    "./refs": {
      "default": "./refs.d.ts"
    }
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^18.19.56",
    "is-published": "^0.2.0",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  }
}
